<Window x:Class="Personas.AñadirNacionalidad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Personas"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Icon="assets/añadir.ico"
        Title="Añadir Nacionalidad" Height="100" Width="300">
    <StackPanel>
        <TextBlock>Nacionalidad</TextBlock>
        <TextBox></TextBox>
        <StackPanel Margin="10" HorizontalAlignment="Right" Orientation="Horizontal">
            <Button Margin="0,0,10,0" IsDefault="True" Click="AceptarButton_Click">Aceptar</Button>
            <Button IsCancel="True">Cancelar</Button>
        </StackPanel>
    </StackPanel>
</Window>
